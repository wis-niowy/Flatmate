{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.js",
        "jquery.slim.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "popper.js@1.14.7",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "umd/popper.min.js",
        "umd/popper.js.map",
        "umd/popper.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "mdbootstrap@4.8.7",
      "destination": "wwwroot/lib/mdbootstrap/"
    },
    {
      "library": "moment.js@2.24.0",
      "destination": "wwwroot/moment.js/"
    }
  ]
}